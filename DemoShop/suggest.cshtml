@using Omikron.FactFinder
@using Omikron.FactFinder.Default
@using Omikron.FactFinder.Raw
@using Omikron.FactFinder.Core
@using Omikron.FactFinder.Core.Server

@{
    Response.ContentType = "text/javascript";
    var parametersHandler = new ParametersHandler();
    var suggestProvider = new HttpDataProvider();
    var suggestAdapter = new RawSuggestAdapter(suggestProvider, parametersHandler);
}

@Html.Raw(suggestAdapter.RawSuggestions)