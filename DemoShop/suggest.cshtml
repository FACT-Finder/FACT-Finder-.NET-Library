@using Omikron.FactFinder
@using Omikron.FactFinder.Default
@using Omikron.FactFinder.Raw
@using Omikron.FactFinder.Core
@using Omikron.FactFinder.Core.Client
@using Omikron.FactFinder.Core.Server

@{
    Response.ContentType = "text/javascript";
    var parametersHandler = new ParametersConverter();
    var suggestProvider = new HttpDataProvider();
    var requestParser = new RequestParser();
    var clientUrlBuilder = new Omikron.FactFinder.Core.Client.UrlBuilder(requestParser);
    var suggestAdapter = new RawSuggestAdapter(suggestProvider, parametersHandler, clientUrlBuilder);
}

@Html.Raw(suggestAdapter.RawSuggestions)